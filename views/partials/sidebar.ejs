<body class="app sidebar-mini">
  <div id="global-loader"></div>

  <div class="page">
    <div class="page-main">

      <!-- SIDEBAR -->
      <div class="sticky">
        <aside class="app-sidebar">
          <div class="main-sidebar-header">
            <a href="/dashboard" class="header-brand">
              <img src="/assets/images/brand-logos/desktop-logo.png"class="desktop-logo" alt="logo">
            </a>
          </div>

          <div class="main-sidebar">
            <ul class="side-menu">

              <li class="slide">
                <a class="side-menu__item" href="/dashboard">
                  <i class="side-menu__icon fe fe-home"></i>
                  <span class="side-menu__label">Dashboard</span>
                </a>
              </li>

              <li class="slide">
                <a class="side-menu__item" href="/caja">
                  <i class="side-menu__icon fe fe-dollar-sign"></i>
                  <span class="side-menu__label">Caja</span>
                </a>
              </li>

              <li class="slide">
                <a class="side-menu__item" href="/bancos">
                  <i class="side-menu__icon fe fe-credit-card"></i>
                  <span class="side-menu__label">Bancos</span>
                </a>
              </li>

            </ul>
          </div>
        </aside>
      </div>

      <!-- CONTENT -->
      <div class="main-content app-content">
        <div class="container-fluid">
          <%- body %>
        </div>
      </div>

    </div>
  </div>
</body>
